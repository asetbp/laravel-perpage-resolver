{
  "name": "roquie/laravel-perpage-resolver",
  "description": "Add a few feature to resolve per_page query parameter globally.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "roquie",
      "email": "roquie0@gmail.com"
    }
  ],
  "require": {
    "illuminate/database": "^5.2|^5.3|^5.4|^5.5",
    "illuminate/pagination": "^5.2|^5.3|^5.4|^5.5",
    "illuminate/support": "^5.2|^5.3|^5.4|^5.5"
  },
  "require-dev": {
    "symfony/var-dumper": "^3.2",
    "phpunit/phpunit": "^6.3",
    "mockery/mockery": "^0.9.9",
    "php-coveralls/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Roquie\\LaravelPerPageResolver\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Roquie\\LaravelPerPageResolver\\PerPageResolverServiceProvider"
      ]
    }
  }
}
